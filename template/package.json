{
  "name": "com.suixingpay.syt",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "patch-package && node node_modules/sensorsdata-analytics-react-native/SensorsDataRNHook.js -run ",
    "android": "react-native run-android --tasks installDevDebug",
    "ios": "react-native run-ios",
    "android-prod": "ENVFILE=.env.prod react-native run-android --tasks installDevDebug",
    "ios-prod": "ENVFILE=.env.prod react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start --reset-cache",
    "pod": "npx pod-install ios --verbose --no-repo-update",
    "build-android-debug": "cd android && ./gradlew assembleDebug",
    "build-android-release": "cd android && ./gradlew assembleDevRelease",
    "build-android-prod-debug": "cd android && ENVFILE=.env.prod ./gradlew assembleProdDebug",
    "build-android-prod-release": "cd android && ENVFILE=.env.prod ./gradlew assembleProdRelease",
    "test": "jest",
    "dev-tool": "node ./dev-tool/server.js",
    "bundle-analysis": "visualize-bundle",
    "code-push-ios": "pushy bundle --platform ios",
    "code-push-android": "pushy bundle --platform android"
  },
